generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model booking {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now())
  room         String
  startingTime DateTime
  endingTime   DateTime
}
